	<queryString language="SQL">
		<![CDATA[SELECT invoice_internal."INV_DATE",
	invoice_internal_customer2."CUST_NAME" AS customer_cust_name,
	invoice_internal_customer2."CUST_POSTCODE" customer_cust_postcode,
	invoice_internal_customer2."CUST_EGHTESADNUMBER" customer_cust_eghtesadnumber,
	invoice_internal_customer2."CUST_ADDRESS" customer_cust_address,
	invoice_internal_customer2."CUST_TEL" customer_cust_tel,
	invoice_internal_customer2."CUST_SABTNUMBER" customer_cust_sabtnumber,
	invoice_internal_customer1."CUST_NAME" AS buyer_cust_name,
	invoice_internal_customer1."CUST_POSTCODE" AS buyer_cust_postcode,
	invoice_internal_customer1."CUST_EGHTESADNUMBER" AS buyer_cust_eghtesadnumber,
	invoice_internal_customer1."CUST_ADDRESS" AS buyer_cust_address,
	invoice_internal_customer1."CUST_TEL" AS buyer_cust_tel,
	invoice_internal_customer1."CUST_TELEX" AS buyer_cust_telex,
	invoice_internal_customer1."CUST_FAX" AS buyer_cust_fax,
	invoice_internal_customer1."CUST_SABTNUMBER" AS buyer_cust_sabtnumber,
	invoice_internal."HAVALEHID",
	invoice_internal."GOODSID",
	invoice_internal."GDSNAME",
	invoice_internal."WEIGHTREAL",
	invoice_internal."GHEMATUNIT",
	invoice_internal."MABLAGHKOL",
	invoice_internal."PAYFORAVAREZMALEYATE",
	invoice_internal."BANKGROUPDESC",
	invoice_internal."LCID",
	invoice_internal_lc."LC_NUMBER"
FROM "TBL_INVOICE_INTERNAL" invoice_internal
	JOIN "TBL_INVOICE_INTERNAL_CUSTOMER" invoice_internal_customer1 ON 
	 invoice_internal_customer1."CUST_ID" = invoice_internal."BUYERID" 
	JOIN "TBL_INVOICE_INTERNAL_CUSTOMER" invoice_internal_customer2 ON 
	 invoice_internal_customer2."CUST_ID" = invoice_internal."CUSTOMERID" 
	left join "TBL_INVOICE_INTERNAL_LC" invoice_internal_lc ON 
	 invoice_internal_lc."LCID" = invoice_internal."LCID" 
WHERE 
	 invoice_internal."ID" = $P{rowId}]]>
	</queryString>
