<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet author="RMazloom" id="1582016166374-261">
        <dropForeignKeyConstraint baseTableName="TBL_WAREHOUSE_ISSUE_CATHODE" constraintName="WarehouseissueCad2shipment"/>
        <dropForeignKeyConstraint baseTableName="TBL_WAREHOUSE_ISSUE_CONS" constraintName="Warehouseissuecons2shipment"/>
        <dropForeignKeyConstraint baseTableName="TBL_WAREHOUSE_ISSUE_MO" constraintName="Warehouseissuemo2lot"/>
        <dropForeignKeyConstraint baseTableName="TBL_WAREHOUSE_ISSUE_MO" constraintName="Warehouseissuemo2shipment"/>

        <dropTable tableName="TBL_WAREHOUSE_ISSUE_CATHODE"/>
        <dropTable tableName="TBL_WAREHOUSE_ISSUE_CONS"/>
        <dropTable tableName="TBL_WAREHOUSE_ISSUE_MO"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-262">
        <dropTable tableName="TBL_SHIPMENT_ASSAY_ITEM"/>
        <dropTable tableName="TBL_SHIPMENT_ASSAY_HEADER"/>
        <dropTable tableName="TBL_SHIPMENT_EMAIL"/>
        <dropTable tableName="TBL_SHIPMENT_MOISTURE_ITEM"/>
        <dropTable tableName="TBL_SHIPMENT_MOISTURE_HEADER"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-263">
        <createTable tableName="TBL_INVOICE_FOREIGN">
            <column name="ID" type="NUMBER(19, 0)">
                <constraints primaryKey="true" primaryKeyName="TBL_INVOICE_FOREIGN_PK"/>
            </column>
            <column name="c_created_by" type="VARCHAR2(255 CHAR)">
                <constraints nullable="false"/>
            </column>
            <column name="d_created_date" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
            <column name="c_last_modified_by" type="VARCHAR2(255 CHAR)"/>
            <column name="d_last_modified_date" type="TIMESTAMP"/>
            <column name="n_version" type="NUMBER(10, 0)">
                <constraints nullable="false"/>
            </column>
            <column name="COPPER" type="FLOAT(24)"/>
            <column name="COPPER_UNIT_PRICE" type="FLOAT(24)"/>
            <column name="SILVER" type="FLOAT(24)"/>
            <column name="SILVER_UNIT_PRICE" type="FLOAT(24)"/>
            <column name="GOLD" type="FLOAT(24)"/>
            <column name="GOLD_UNIT_PRICE" type="FLOAT(24)"/>
            <column name="GROSS" type="FLOAT(24)"/>
            <column name="INVOIC_DATE" type="VARCHAR2(20 CHAR)"/>
            <column name="INVOICE_NO" type="VARCHAR2(100 CHAR)"/>
            <column name="INVOICE_TYPE" type="VARCHAR2(20 CHAR)"/>
            <column name="INVOICE_VALUE" type="FLOAT(24)"/>
            <column name="INVOICE_VALUE_CUR" type="VARCHAR2(10 CHAR)"/>
            <column name="MOLYBDENUM_UNIT_PRICE" type="FLOAT(24)"/>
            <column name="MOLYBDENUM" type="FLOAT(24)"/>
            <column name="NET" type="FLOAT(24)"/>
            <column name="PAID_PERCENT" type="FLOAT(24)"/>
            <column name="PAID_STATUS" type="VARCHAR2(10 CHAR)"/>
            <column name="PROCESS_ID" type="VARCHAR2(255 CHAR)"/>
            <column name="RC_AG" type="FLOAT(24)"/>
            <column name="RC_AU" type="FLOAT(24)"/>
            <column name="RC_CU" type="FLOAT(24)"/>
            <column name="TREAT_COST" type="FLOAT(24)"/>
            <column name="UNIT_PRICE" type="FLOAT(24)"/>
            <column name="UNIT_PRICE_CUR" type="VARCHAR2(10 CHAR)"/>
            <column name="SHIPMENT_ID" type="NUMBER(19, 0)"/>
        </createTable>
        <createSequence sequenceName="SEQ_INVOICE_FOREIGN"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-264">
        <dropSequence sequenceName="SEQ_INVOICE"/>
        <dropAllForeignKeyConstraints baseTableName="TBL_INVOICE"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-265">
        <dropAllForeignKeyConstraints baseTableName="TBL_INVOICE_MOLYBDENUM"/>
        <dropTable tableName="TBL_INVOICE_MOLYBDENUM"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-266">
        <createSequence sequenceName="SEQ_INVOICE_ITEM"/>
    </changeSet>

    <changeSet author="RMazloom" id="1582016166374-267">
        <dropTable tableName="TBL_INVOICE_ITEM"/>
    </changeSet>
</databaseChangeLog>
