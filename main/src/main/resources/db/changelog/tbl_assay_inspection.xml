<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet author="MShahabi" id="1592814189336-239">
        <addColumn tableName="TBL_ASSAY_INSPECTION">
            <column name="C_LAB_NAME" type="varchar2(255 CHAR)"/>
        </addColumn>
    </changeSet>

    <changeSet author="MShahabi" id="1592814189336-240">
        <addColumn tableName="TBL_ASSAY_INSPECTION">
            <column name="C_LAB_PLACE" type="varchar2(255 CHAR)"/>
        </addColumn>
    </changeSet>

    <changeSet author="MShahabi" id="1799887766554-001">
        <dropForeignKeyConstraint baseTableName="TBL_ASSAY_INSPECTION" constraintName="FK_ASSAYINSPECTION2REMITTANCEDETAILBYREMITTANCEDETAILID"/>
        <dropColumn columnName="F_REMITTANCE_DETAIL_ID" tableName="TBL_ASSAY_INSPECTION"/>
    </changeSet>

    <changeSet author="Reza Mazloom" id="mazloom_002">
        <renameColumn tableName="TBL_ASSAY_INSPECTION" oldColumnName="F_ITEM_ELEMENT_ID" newColumnName="F_MATERIAL_ELEMENT_ID"/>
    </changeSet>

    <changeSet author="Reza Mazloom" id="mazloom_003">
        <dropForeignKeyConstraint baseTableName="TBL_ASSAY_INSPECTION" constraintName="fk_AssayInspection2itemElementByItemElementId"/>
        <addForeignKeyConstraint baseTableName="TBL_ASSAY_INSPECTION" baseColumnNames="F_MATERIAL_ELEMENT_ID"
                                 constraintName="fk_AssayInspection2materialElementByMaterialElementId" referencedTableName="TBL_WARH_MATERIAL_ELEMENT"
                                 referencedColumnNames="ID"/>
    </changeSet>

    <changeSet id="MShahabi" author="1799887766554-004">
        <modifyDataType columnName="N_VALUE" newDataType="NUMBER(10, 5)" tableName="TBL_ASSAY_INSPECTION"/>
    </changeSet>

</databaseChangeLog>
